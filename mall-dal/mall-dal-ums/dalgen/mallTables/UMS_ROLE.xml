<!DOCTYPE table SYSTEM "../config/table-config-1.0.dtd">
<table sqlname="ums_role"
       physicalName="ums_role" remark="角色表">
    <!--  特殊字符说明 &lt;&gt; <> -->
    <!-- baseSql option中 select * 会自动替换为 include -->
    <sql id="Base_Column_List">
        ID,CREATOR,MODIFIER,NAME,REMARK
        ,STATUS,IS_DELETED,SORT,GMT_CREATE,GMT_MODIFY
    </sql>

    <!-- baseSql option中 select sf.* 会自动替换为 include -->
    <sql id="Base_SF_Column_List">
        sf.ID,sf.CREATOR,sf.MODIFIER,sf.NAME,sf.REMARK
        ,sf.STATUS,sf.IS_DELETED,sf.SORT,sf.GMT_CREATE,sf.GMT_MODIFY
    </sql>

    <operation name="insert" paramtype="object" remark="插入表:ums_role"
               autoGen="true">
        <selectKey resultType="java.lang.Long" keyProperty="id" order="AFTER">
            SELECT
            LAST_INSERT_ID()
        </selectKey>
        <![CDATA[
    INSERT INTO ums_role(
        ID
        ,CREATOR
        ,MODIFIER
        ,NAME
        ,REMARK
        ,STATUS
        ,IS_DELETED
        ,SORT
        ,GMT_CREATE
        ,GMT_MODIFY
    )VALUES(
         null
        , #{creator,jdbcType=BIGINT}
        , #{modifier,jdbcType=BIGINT}
        , #{name,jdbcType=VARCHAR}
        , #{remark,jdbcType=VARCHAR}
        , #{status,jdbcType=VARCHAR}
        , 0
        , #{sort,jdbcType=INTEGER}
        , now()
        , #{gmtModify,jdbcType=TIMESTAMP}
    )
    ]]>
    </operation>

    <!-- foreach 可以自定义类型，paramtype="primitive" foreach->javatype="自己书写的类"  -->
    <!-- 只有一个参数且为List时必须将参数命名为list -->
    <operation name="insertBatch" paramtype="objectList"
               remark="批量插入表:ums_role" autoGen="true">
        INSERT INTO ums_role(
        ID
        ,CREATOR
        ,MODIFIER
        ,NAME
        ,REMARK
        ,STATUS
        ,IS_DELETED
        ,SORT
        ,GMT_CREATE
        ,GMT_MODIFY
        )VALUES
        <foreach collection="list" item="item" separator=",">
            (
            null
            , #{item.creator,jdbcType=BIGINT}
            , #{item.modifier,jdbcType=BIGINT}
            , #{item.name,jdbcType=VARCHAR}
            , #{item.remark,jdbcType=VARCHAR}
            , #{item.status,jdbcType=VARCHAR}
            , 0
            , #{item.sort,jdbcType=INTEGER}
            , now()
            , #{item.gmtModify,jdbcType=TIMESTAMP}
            )
        </foreach>
    </operation>

    <!-- 不推荐使用全 update 有需要自己打开
<operation name="update" paramtype="object" remark="更新表:ums_role"  autoGen="true">
<![CDATA[
UPDATE ums_role
SET
    MODIFIER        = #{modifier,jdbcType=BIGINT}
    ,NAME            = #{name,jdbcType=VARCHAR}
    ,REMARK          = #{remark,jdbcType=VARCHAR}
    ,STATUS          = #{status,jdbcType=VARCHAR}
    ,IS_DELETED      = #{isDeleted,jdbcType=CHAR}
    ,SORT            = #{sort,jdbcType=INTEGER}
    ,GMT_MODIFY      = #{gmtModify,jdbcType=TIMESTAMP}
WHERE
    ID              = #{id,jdbcType=BIGINT}
]]>
</operation>
-->
    <operation name="deleteById"
               remark="根据主键删除数据:ums_role">
        <![CDATA[
                DELETE FROM ums_role
                WHERE
                    ID = #{id,jdbcType=BIGINT}
                ]]>
    </operation>

    <operation name="getById" multiplicity="one"
               remark="根据主键获取数据:ums_role">
        SELECT *
        FROM ums_role
        WHERE
        <![CDATA[
                    ID = #{id,jdbcType=BIGINT}
                ]]>
    </operation>

</table>
